{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "preserveConstEnums": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noEmitOnError": true,
        "strictNullChecks": true,
        "target": "es2018",
        "sourceMap": true,
        "esModuleInterop": true,
        "resolveJsonModule": true
    },
    "files": [
        "index.ts"
    ]
}
